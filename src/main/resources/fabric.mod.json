{
  "schemaVersion": 1,
  "id": "neutrino",
  "version": "${version}",

  "name": "Neutrino",
  "description": "A Light, Miscellaneous Quark Spin-Off Fabric Mod for Minecraft, inspired by The Creeper's Compendium!",
  "authors": [
    "FrostWizard4",
    "The_Fresh_Potato",
    "Starkiller580",
    "eliask2k",
    "Yama_Lama",
